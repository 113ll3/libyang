module ext-def {
  yang-version 1.1;
  namespace "urn:ext-def";
  prefix e;

  revision 2016-01-18;

  extension a;

  extension b {
    argument x;
  }

  extension c {
    argument y {
      yin-element true;
    }
  }

  typedef mystring {
    type string;
  }

  leaf l1 {
    type string;
  }
  leaf l2 {
    type string;
    mandatory true {
      e:b "one";
      e:c "one";
    }
  }
  leaf-list ll1 {
    type uint8;
  }
  list lst1 {
